{
    "name": "friendsofouro/kurrentdb-bundle",
    "description": "KurrentDB integration for Symfony 7.x or later",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "cqrs",
        "eventsourcing",
        "KurrentDB"
    ],
    "authors": [
        {
            "name": "D. Bellettini",
            "homepage": "https://bellettini.eu/"
        },
        {
            "name": "Nicol√≤ Pignatelli",
            "email": "pignatelli.nicolo@gmail.com",
            "homepage": "https://nicolopignatelli.me/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/FriendsOfOuro/kurrentdb-bundle/graphs/contributors"
        }
    ],
    "require": {
        "php": "^8.4",
        "friendsofouro/kurrentdb": "^0.2",
        "symfony/framework-bundle": "^7.3 || ^8.0",
        "symfony/yaml": "^7.3 || ^8.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.48",
        "friendsofphp/php-cs-fixer": "^3.87",
        "phpunit/phpunit": "^12.3"
    },
    "replace": {
        "dbellettini/eventstore-client-bundle": "self.version",
        "dbellettini/kurrentdb-bundle": "self.version"
    },
    "autoload": {
        "psr-4": {
            "EventStore\\Bundle\\ClientBundle\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.4.x-dev"
        }
    },
    "replaces": {
        "friendsofouro/geteventstore-bundle": "self.version"
    }
}
